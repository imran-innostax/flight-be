{
	"name": "tavajs",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "npm run pretty && npm run eslint",
		"start": "npm run pretty && npm run eslint && npm run start-watch",
		"start-watch": "concurrently --kill-others \"npm run prettier-watch\" \"npm run eslint-watch\" \"pm2 start index.js\"",
		"pretty": "npx prettier --write \"**/*\"",
		"prettier-watch": "onchange \"**/*\" -- npx prettier --write {{changed}}",
		"eslint": "npx eslint",
		"eslint-watch": "onchange \"**/*.js\" -- npx eslint",
		"build": "echo \"There is no build command for backend\"",
		"ci": "yarn install --frozen-lockfile",
		"start-docker": "npx prisma format && npx prisma db pull && npx prisma generate && npm start"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^4.11.0",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"express": "^4.18.1",
		"axios": "^1.4.0",
		"@types/amqplib": "^0.10.1",
		"amqplib": "^0.10.3",
		"ejs": "^3.1.9",
		"html-to-text": "^9.0.5",
		"aws-sdk": "^2.1450.0",
